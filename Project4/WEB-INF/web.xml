<web-app>

   <!-- General description of your Web application -->
   <display-name>
      CNT 4714 Spring 2018 Servlet/JSP Home Page
   </display-name>

   <description>
      This is the Web application in which we 
      demonstrate our JSP and Servlet examples.
   </description>

   <!-- Servlet definitions -->
    <servlet>
      <servlet-name>IndexSession</servlet-name>
      <description>A simple servlet that handles an HTTP get request.</description>
      <servlet-class>IndexSession</servlet-class>
   </servlet>

   <!-- Servlet mappings -->
   <servlet-mapping>
      <servlet-name>IndexSession</servlet-name>
      <url-pattern>/IndexSession</url-pattern>
   </servlet-mapping>


</web-app>

